{
    "source": {
        "include": [
            "src/js",
            "README.md"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "applicationName": "tui-grid",
        "name": "Grid",
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "logo": {
            "url": "https://cloud.githubusercontent.com/assets/12269563/20029815/01133928-a39a-11e6-80f3-12500a91c755.png",
            "width": "150px",
            "height": "13px",
            "link": "https://github.com/nhnent/tui.jsdoc-template"
        },
        "linenums": false
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "opts": {
        "recurse": true,
        "template": "node_modules/tui-jsdoc-template",
        "name": "Toast UI Grid",
        "destination": "doc",
        "tutorials": "examples"
    }
}
